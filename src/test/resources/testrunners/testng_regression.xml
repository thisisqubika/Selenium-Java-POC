<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Regression PW Suite">

    <listeners>
        <listener class-name="extentReport.FareyeListener" />

    </listeners>
    <test name="PractisTest Test Microsoft Edge">
        <parameter name="headless" value="false"/>
        <parameter name="browser" value="msedge" />
        <classes>
            <class name="Test.AutomationExerciseLoginTest" />

        </classes>

    </test>
    <test name="PractisTest Test Mozilla Firefox">
        <parameter name="headless" value="false"/>
        <parameter name="browser" value="firefox" />
        <classes>
            <class name="Test.AutomationExerciseLoginTest" />

        </classes>

    </test>
    <test name="PractisTest Test Google Chrome">
        <parameter name="headless" value="false"/>
        <parameter name="browser" value="chrome" />
        <classes>
            <class name="Test.AutomationExerciseLoginTest" />

        </classes>

    </test>
